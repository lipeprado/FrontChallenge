{
  "name": "frontchallenge",
  "description": "A FrontEnd Challenge with ReactJS and  Marvel API",
  "author": "Filipe Prado",
  "license": "ISC",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/hcoura/react_redux_starter"
  },
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel dev-server lint:watch",
    "dev-server": "babel-node tools/devServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf dist",
    "prebuild": "npm-run-all clean-dist lint",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js",
    "remove-gzip": "node_modules/.bin/rimraf dist_gzip",
    "precompress": "npm run remove-gzip && mkdir dist_gzip",
    "compress": "babel-node tools/compress.js"
  },
  "devDependencies": {
    "axios": "^0.17.1",
    "eslint": "^4.11.0",
    "eslint-watch": "^3.1.3",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "file-loader": "^1.1.5",
    "jquery": "^3.2.1",
    "node-sass": "^4.7.1",
    "npm-run-all": "^4.1.2",
    "open": "^0.0.5",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "tether": "^1.4.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0"
  }
